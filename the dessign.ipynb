    "import turtle\n",
    "import colorsys\n",
    "\n",
    "t = turtle.Turtle()\n",
    "s = turtle.Screen()\n",
    "s.bgcolor('black')\n",
    "t.speed(0)\n",
    "n = 50\n",
    "h = 0\n",
    "\n",
    "for i in range(200):\n",
    "    c = colorsys.hsv_to_rgb(h,1,0.8)\n",
    "    h = h+1/n\n",
    "    t.color(c)\n",
    "    t.forward(i*2)\n",
    "    t.left(145)\n",
    "turtle.done()\n"
   ]
